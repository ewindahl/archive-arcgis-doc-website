        </figure>
        <div class="card-content">
          <h4>AppStudio-Windows-x64</h4>
          <p class="small-grey file-size">180 MB</p>
          <%= generateVersionDropDown("appstudio~secured~windows64") %>
        </div>
      </div>
      <div class="card card-shaped block">
        <figure class="card-image-wrap">
          <img src="../img/icons/win64.png" alt="AppStudio-Windows-x6" class="card-image">
