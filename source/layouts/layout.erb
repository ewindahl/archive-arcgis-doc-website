     <!-- top navigation collapses to drawer on mobile -->
    <div class="drawer drawer-left js-drawer" data-drawer="top-nav">
        <nav class="drawer-nav">
          <aside class="side-nav">
            <h2 class="side-nav-title">Arc<span class="avenir-demi">GIS</span></h2>
            
            <form class="gutter-left-half gutter-right-half leader-half trailer-half" method="GET" action="/search">
              <input type="search" name="q" value="" placeholder="<%= current_page.data.searchDfltStr %> Search" class="trailer-0">
              <input type="hidden" name="collection" value="help" />
              <input type="hidden" name="product" value="<%= current_page.data.productMeta %>" />
            </form>
    
            <%= partial "_partial/site_header_nav", :locals => { :navLinkClass => "side-nav-link" } %>
          </aside>
        </nav>
    </div>

    <!-- user navigation collapses to drawer on mobile -->
    <div class="drawer drawer-right js-drawer logged-in-navigation" data-drawer="user-nav">
      <nav class="drawer-nav">
        <aside class="side-nav">
          <h4 class="side-nav-title padding-leader-half padding-trailer-half" data-full-name></h4>
          <h6 class="side-nav-title font-size--2" data-username></h6>
          <a class="side-nav-link icon-ui-sign-out js-log-out" id="agolLogout" href="#"><%= t("site.nav.sign_in_logout") %></a>
        </aside>
      </nav>
    </div>
<!-- End of drawer -->


    <div class="wrapper">
      <!--googleoff: all-->

      <%= partial "_partial/site_app_header" %>

       <section class="expand js-expand  panel panel-no-padding" data-expand="search">
              <div class="grid-container padding-trailer-half padding-leader-1 padding-trailer-1">
                
          <form method="GET" action="//www.arcgis.com/home/search.html" class="column-24">
            <label>Search ArcGIS
              <a href="#" class="icon-ui-close js-expand-toggle right" data-expand="search"></a>
              <div class="search-bar">
                <input type="search" name="q" placeholder="Search">
                <button type="submit" class="search-submit icon-ui-search"></button>
              </div>
            </label>
          </form>

    
        </div>
      </section>
      <!--googleon: all-->

      <div id="skip-to-content">
        <%= yield %>
      </div>

<% if current_page.data.blog_url or current_page.data.forum_url or current_page.data.agol_gallery %>
	<script>
	  $(window).load(function() {
		  <% if current_page.data.blog_url %>
		  $("#blog").rssFeed({
			url:"<%= current_page.data.blog_url %>", 
			max: 2, 
			layout: 0
		  });
		  <% end %>
		  <% if current_page.data.forum_url %>
		  $("#forum").rssFeed({
			url:"<%= current_page.data.forum_url%>", 
			max: 2,        
			layout: 1
		  });
		  <% end %>
		  <% if current_page.data.agol_gallery %>
			  <% if !current_page.data.agol_url %>
					$("#gallery-cont").agolGallery({
					    url: "http://www.arcgis.com/sharing/rest/search?q=%20group%3A5e79591f54564399b99c76ec58a245ab%20-type:%22Code%20Attachment%22%20-type:%22Featured%20Items%22%20-type:%22Symbol%20Set%22%20-type:%22Color%20Set%22%20-type:%22Windows%20Viewer%20Add%20In%22%20-type:%22Windows%20Viewer%20Configuration%22%20%20-type:%22Code%20Attachment%22%20-type:%22Featured%20Items%22%20-type:%22Symbol%20Set%22%20-type:%22Color%20Set%22%20-type:%22Windows%20Viewer%20Add%20In%22%20-type:%22Windows%20Viewer%20Configuration%22%20&num=10&sortField=modified&sortOrder=desc&f=json",
					   max:8
					  });
				<% else %>
		  			$("#gallery-cont").agolGallery({
					   url: "<%= current_page.data.agol_url %>",
					   max:8
					  });
				<% end %>
		  <% end %>
	  });
	</script>
	<% end %>

  </div>
    
  

    <footer class="footer leader-0 link-dark-gray" role="contentinfo">
      <div class="grid-container">
          <nav class="column-6">
            <h6>ArcGIS</h6>
            <ul class="list-plain">
              <%= partial "_partial/site_footer_nav" %>
            </ul>
          </nav>

          <%= partial "_partial/site_footer_common" %>
      </div>
    </footer>

    <%= javascript_include_tag "search/searchform.js" %>
    <%= javascript_include_tag "/cdn/js/sitewide.js" %>
    <%= javascript_include_tag "/cdn/js/link-modifier.js" %>


