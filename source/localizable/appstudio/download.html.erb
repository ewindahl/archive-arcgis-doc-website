                        <p class="small-grey">229 MB</p>
                        <%= generateVersionDropDown("appstudio~secured~linux") %>
                    </div>
                </div>
            </div>
        </div>
        <p>&nbsp;</p>
        <div id="player" class="header leader trailer">
            <h4 class="gray trailer">Use AppStudio Player to preview apps on your devices before you build!</h4>
        </div>
        <div class="row downloads">
            <div class="column-6 tablet-column-23">
                <div id="player-win64" class="card">
