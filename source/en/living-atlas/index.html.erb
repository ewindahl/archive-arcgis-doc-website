---
title: Living Atlas of the World
css:
  - gallery

js:
  - gallery/gallery-init
  - gallery/galleryshell-default
  - gallery/gallerydisplay-default


metatags:
  - <META name="description" content="Living Atlas of the World"><META name="last-modified" content="2015-06-25"><META name="product" content="living-atlas,arcgis-online"><META name="sub_category" content=""><META name="search-collection" content="gallery"><META name="content_type_label" content="Living Atlas Gallery"><META name="sub_category_label" content=""><META name="search-category" content="gallery-itemtype"><META name="subject_label" content="ARCGIS ONLINE"><META name="version" content=""><META name="content_type" content="Gallery Item"><META name="subject" content="ARCGIS ONLINE"><META name="content-language" content="en">

gallery_data_path: data/gallery/living-atlas-agol=en.xml
expanded_gallery: true
---

<header class="sub-nav sub-nav-blue">
  <div class="grid-container">
    <div class="column-24">
      <div style="display:table;" class="sdk-home-banner">
        <div class="sdk-home-banner-content">
          <h1 class="sub-nav-title leader-1 text-white"><a class="text-white" href="/en/living-atlas/">Living Atlas of the World</a></h1>
        </div>
        <!-- Placeholder for image in blue banner-->
		  <div class="search-bar">
			 <form id="gallerySearchForm" target="_top" autocomplete="off">
			  <input type="search" name="q" id="query" class="leader" placeholder="">
			  <input type="hidden" name="collection" value="help" />
			  <input type="hidden" name="language" value="<%= current_language %>" />
			  <h5 id="gl-cl-btn" class="icon-cancel"></h5>
			  <button class="search-submit icon-ui-search" type="submit"></button>
			</form>
		  </div>
      </div>
    </div>
    <div class="column-24">
      <nav class="sub-nav-list">
		  <a class="sub-nav-link" href="/en/living-atlas/about/"> Home </a>
        <a class="sub-nav-link" href="/en/living-atlas/use/"> Use </a>
        <a class="sub-nav-link" href="/en/living-atlas/contribute/"> Contribute </a>
        <a class="sub-nav-link is-active" href="/en/living-atlas/"> Browse </a>
      </nav>
    </div>
  </div>
</header>


<div class="panel panel-white panel-no-border panel-no-padding">
	<div class="content container leader-3">
  
  <%= partial :"_partial/galleryFilters" %>
    <div id="reference-content">

  

    <div id="gl-display">
     <div id="spinner" style="display: none;">
		<img alt="loading" src="/img/gallery/ajax-loader.gif">
     </div>
     
     <!-- Result area -->
     <div id="gl-content" class="display1">
     </div>
     <!-- End of Result -->

     
    </div>
	 
     <div class="clear"></div>
	  
	  <div class="profilePopup">
        <a href="JavaScript:void(0);"><span class="right icon-ui-close">Close</span></a>
        <div class="clear"></div>
        <div class="spinner">
            <div>
              <img alt="loading" src="/img/gallery/ajax-loader.gif">
            </div>
        </div>
        <div class="profileDetails hide column-24 left" style="position: relative; top: 0px; left: 0px;">
            <div class="column-2">
              <div class="itemThumbnailContainer"></div>
            </div>
            <div class="column-7">
              <h4 class="profile-name blue"></h4>
              <div class="drop-shadow">
                <p class="profile-content">
                  
                </p>
              </div>
              <div class="profileLinks right">
                <a class="profileLink" target="_blank" href="#">Profile</a>
                <a class="profileItemsLink" target="_blank" href="#">Items</a>
                <a class="profileGroupLink" target="_blank" href="#">Groups</a>
              </div>
            </div>
        </div>
      </div>
    </div> <!-- End of reference content -->
	 
	 
	 
	</div>
</div>

	
