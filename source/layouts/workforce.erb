    <div class="drawer drawer-left js-drawer" data-drawer="top-nav">
        <nav class="drawer-nav">
          <aside class="side-nav">
            <h2 class="side-nav-title">Workforce for ArcGIS</h2>
            
            <form class="margin-left-half margin-right-half leader-half trailer-half" method="GET" action="/search">
              <input type="search" name="q" value="" placeholder="<%= current_page.data.searchDfltStr %> Search" class="trailer-0">
              <input type="hidden" name="collection" value="help" />
              <input type="hidden" name="product" value="workforce-ipad" />
            </form>
    
            <a data-langlabel="myprojects" href="//workforce.arcgis.com/projects" class="side-nav-link"><%= t("site.nav.workforce.myprojects") %></a>
