---
layout: search

title: Search Result
css:
  - searchResult


js:
  - sitecfg
  - /cdn/js/libs/underscore
  - /cdn/js/libs/backbone
  - search/jxon
  - search/searchcfg
  - search/gsasearch
  - marketplace/init


metatags:
  - <META name="robot" content="NOINDEX,NOFOLLOW">

languageSelector: "No"

mobile: true
root: true

---

<!-- Page Content -->

<div class="grid-container leader-2 trailer-1 inithide">
  <div class="column-6 trailer-2">
    <aside id="searchFilter" role="tablist" aria-live="polite" class="js-accordion accordion column-6"></aside>
  </div>
  <main class="column-17 pre-1">
    <div class="panel search-box wf-nav">
      <div id="searchBox" class="wf-nav search-bar column-17 trailer-0 leader-0 padding-right-3 padding-left-1" style="height:55px">
        <form id="gsaSearchForm" class="input-group">
          <input type="search" placeholder="Search doc.arcgis.com help" data-langlabel="search-arcgis-server-content" name="q" id="query" class="input-group-input">
          <span class="input-group-button">
            <button class="icon-ui-search icon-ui-flush" type="submit"></button>
          </span>
        </form>
      </div>
    </div>

    <div class="row search-nav">
      <div id="searchStat" class="column-9"></div>
      <div class="pre-1 column-2"></div>
      <div id="searchLang" class="pre-1 column-11"></div>
    </div>

    <hr />

    <div id="results">
      <div id="spinner" class="loader is-active padding-leader-3 padding-trailer-3">
        <div class="loader-bars"></div>
        <div class="loader-text">Loading</div>
      </div>

      <!-- Result area -->
      <div id="searchResult" class="display3">
        <div class="clear"></div>
      </div>
      <!-- End of Result -->
    </div>

    <div id="searchPager">
    </div>
  </main>

</div>
