        </aside>
      </nav>
    </div>
<!-- End of drawer -->


    <div class="wrapper workforce">
      <!--googleoff: all-->

      <header class="top-nav" data-skin="app">
        <a class="skip-to-content" href="#skip-to-content">Skip To Content</a>
		  
		  
			  <div class="panel panel-white panel-no-border panel-no-padding header-panel">
				<div class="tablet-hide">
				  <a href="//workforce.arcgis.com" class="top-nav-title">
					<img width="24" height="24" style="" class="app-logo" src="/assets/img/app-icons/workforce-app.png" alt="Workforce"> <span>Workforce for ArcGIS</span>
				  </a>
				</div>
		  
		  
		   <div class="tablet-show">
				<a href="//workforce.arcgis.com" class="top-nav-title icon-ui-menu top-nav-title js-drawer-toggle tablet-show" data-drawer="top-nav">
					<img width="24" height="24" style="" class="app-logo" src="/assets/img/app-icons/workforce-app.png" alt="Workforce"> <span>Workforce for ArcGIS</span>
				</a>
		  </div>

            <nav role="navigation" class="top-nav-list left tablet-hide">
              <a data-langlabel="myprojects" href="//workforce.arcgis.com/projects" class="top-nav-link"><%= t("site.nav.workforce.myprojects") %></a>
            </nav>
            
          
            <div class="lang-block dropdown js-dropdown right margin-left-1 top-nav-list right"></div>

            <div class="margin-left-1 top-nav-list right hide-if-logged-in logged-out-navigation ">
