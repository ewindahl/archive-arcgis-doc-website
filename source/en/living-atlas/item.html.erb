---
title: Living Atlas of the World
css:
  - gallery-item


js:
  - gallery/gallery-init
  - gallery/galleryItemDetails

metatags:
  - <META name="description" content=""><META name="last-modified" content=""><META name="product" content="living-atlas,arcgis-online"><META name="sub_category" content=""><META name="search-collection" content="gallery"><META name="content_type_label" content="Gallery Item"><META name="sub_category_label" content=""><META name="search-category" content="gallery-itemtype"><META name="subject_label" content="ARCGIS ONLINE"><META name="version" content=""><META name="content_type" content="Gallery Item"><META name="subject" content="ARCGIS ONLINE"><META name="content-language" content="en">

mobile: true

---
<header class="sub-nav sub-nav-blue">
  <div class="grid-container">
    <div class="column-24">
      <div style="display:table;" class="sdk-home-banner">
        <div class="sdk-home-banner-content">
          <h1 class="sub-nav-title leader-1 text-white"><a class="text-white" href="/en/living-atlas/">Living Atlas of the World</a></h1>
        </div>
        <!-- Placeholder for image in blue banner-->
		  <div class="search-bar">
			 <form id="gallerySearchForm" target="_top" autocomplete="off">
			  <input type="search" name="q" id="query" class="leader" placeholder="Search the Living Atlas">
			  <input type="hidden" name="collection" value="help" />
			  <input type="hidden" name="language" value="<%= current_language %>" />
			  <h5 id="gl-cl-btn" class="icon-cancel"></h5>
			  <button class="search-submit icon-ui-search" type="submit"></button>
			</form>
		  </div>
      </div>
    </div>
    <div class="column-24">
      <nav class="sub-nav-list">
		  <a class="sub-nav-link" href="/en/living-atlas/about/"> Home </a>
        <a class="sub-nav-link" href="/en/living-atlas/use/"> Use </a>
        <a class="sub-nav-link" href="/en/living-atlas/contribute/"> Contribute </a>
        <a class="sub-nav-link is-active" href="/en/living-atlas/"> Browse </a>
      </nav>
    </div>
  </div>
</header>

<!-- Page Content -->
<div class="map-container">
  <div class="map-title"></div>
		<iframe id="mapIframe" width="100%" height="600" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src=""></iframe>
</div>		

<div class="grid-container">
<div class="column-24 container item-details">
		<div class="row">	
			<div class="column-10">
			<a name="contentArea"></a>
			<!-- Need to derive H2 below from title in the iteminfo file -->
			<h2 id="itemTitle" class="no-leader no-trailer gray"></h2>
					
			</div>
			<div class="column-6 text-right">
			<!-- Sharing links -->
					<div data-action="shareSocial" class="trailer-0 leader-half inline-block if-can-share-social js-action-shareSocial social" id="socialLinks">
            				
					<a href="http://www.arcgis.com/sharing/rest/content/items/itemId-placeholder/comments?f=rss" target="_blank" class="icon-social-rss"> </a>			
					<a href="http://www.arcgis.com/home/socialnetwork.html?t=item-title-placeholder&amp;n=tw&amp;u=http://www.arcgis.com/home/item.html?id=itemId-placeholder" target="_blank" class="icon-social-twitter margin-left-quarter"> </a>					
					<a href="https://plus.google.com/share?url=http://www.arcgis.com/home/item.html?id=itemId-placeholder" target="_blank" class="icon-social-google-plus margin-left-quarter"> </a>
					<a href="http://www.arcgis.com/home/socialnetwork.html?t=item-title-placeholder&amp;n=fb&amp;u=http://www.arcgis.com/home/item.html?id=itemId-placeholder" target="_blank" class="icon-social-facebook js-open-facebook margin-left-quarter"> </a>
				</div>
				
				
			</div>
			<div class="column-8">
				<!-- Ratings and Views -->
				<div class="ratings">
					<!-- Need to derive actual ratings and views for this item -->
					<span class="icon-ui-favorites icon-ui-gray"></span>
					<span class="icon-ui-favorites icon-ui-gray"></span>
					<span class="icon-ui-favorites icon-ui-gray"></span>
					<span class="icon-ui-favorites icon-ui-gray"></span>
					<span class="icon-ui-favorites icon-ui-gray"></span>
					<!--<small id="ratingsAndView">(4 ratings; 1,257,961 views)</small>-->
					<span class="gutter-left-half rating" id="ratingsAndView">( 0 ratings; 12587765 views)</span>
				</div>

			</div>
		</div>		

			  
		
		  
		  <div class="row">
				<main class="column-16">
				<!-- The ratings?f=pjson file contains username & dates -->
				<p id="mapBy"></p>
				<!-- Need to derive p below from snippet in the iteminfo file -->
				<p id="snippet"></p>	
				<!-- Need to derive links below from item info -->
				<p id="downloadBtns"></p>
				
				
				<hr />
				
				<h3>About this map and data</h3> 
				
				<div id="description" class="trailer-2" >
				<!-- This comes directly from the description tag in the iteminfo.xml file -->
				</div>
				
				<a name="constraints"></a><h3 class="leader text-rule">Access and use constraints</h3>
				<div id="use-constraints" class="trailer-2">
				<!-- This comes directly from the licenseinfo tag in the iteminfo.xml file -->
				</div>
				
				<div class="layers">
				<h3 class="leader text-rule">Layers</h3>
				
					<p>The map contains these layers.</p>
					<div id="map-contents-layers">
					<!-- This comes from items/<itemid>/data -->
					</div>
				</div>
				
				<div class="extent">
				<h3 class="leader text-rule">Extent</h3>
				<div id="map-extent">
				<!-- This comes directly from the extent tag in the iteminfo.xml file -->
				<!-- xmin=left, xmax=right, ymin=bottom, ymax=top -->
				<p>&nbsp;</p>
				</div>
				</div>

				
				
								
				<h3 class="leader text-rule">Tags</h3>
				<!-- This comes from tag values in the iteminfo file -->
				<div id="map-tags" class="map-tags">
					
				</div>
				
				</main>
				<aside class="column-8">
				
				<div id="agol-thumbnail" style="display:none"></div>
					
				<div class="panel white">
					<h3>Credits</h3>
					<div id="map-credits">
					<!-- This comes from accessinformation value in the iteminfo file -->
					<p>&nbsp;</p>
					</div>
				</div>
				<p>&nbsp;</p>
					<!-- Comment Feed -->
					<div class="panel">
					<h3>Comments on this map</h3>
					  <div id="comments">
						<!-- blog goes here -->
					  </div>
					  <a id="addCommentBtn" class="btn light" href="">Add a comment</a>
					</div>
					
					
				</aside>

			</div>

		</div>
	</div> <!-- End of grid container -->

	<script src="/assets/js/gallery.comments.js" type="text/javascript"></script>