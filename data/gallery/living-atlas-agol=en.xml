	  <value id="10" label="Water" metacontent="water"/>
    </filter>
    <filter id="6" label="Earth Observations" metaname="earth-observation">
      <value id="1" label="None" metacontent="earth observations"/>
    </filter>
    <filter id="7" label="Urban Systems" metaname="urban">
   	  <value id="1" label="3D Cities" metacontent="3D"/>
	  <value id="2" label="Movement" metacontent="movement"/>
	  <value id="3" label="Parcels" metacontent="parcels"/>
	  <value id="4" label="People" metacontent="people"/>
	  <value id="5" label="Planning" metacontent="planning"/>
      <value id="6" label="Public" metacontent="public"/>
	  <value id="7" label="Work" metacontent="work"/>
    </filter>
	<filter id="8" label="Transportation" metaname="transportation">
      <value id="1" label="None" metacontent="transportation"/>
    </filter>
	<filter id="9" label="Boundaries and Places" metaname="boundaries">
	  <value id="1" label="Boundaries" metacontent="boundaries"/>
	  <value id="2" label="Places" metacontent="places"/>
    </filter>
    <filter id="10" label="Story Maps" metaname="storymaps">
      <value id="1" label="None" metacontent="story map"/>
    </filter>
    <filter id="11" label="Scenes" metaname="scenes">
      <value id="1" label="None" metacontent="scenes"/>
    </filter>
	
