<?xml version="1.0" encoding="utf-8"?>
<gallerycfg
    ouputpath="\\$host\$rootdir\en\gallery\living-atlas"
    search-collection="gallery"
    search-category="livingatlas"
    additional-category="storymaps" 
    alias="livingatlas">

  <view displayn="30"
        displaymode="1"
        showContributor="1"
        errormsg ="Please try again."
        emptymsg ="No results found."
		searchmsg="Search Living Atlas Gallery" />
  
  <filters>
    <filter id="1" label="Imagery" metaname="imagery">
	  <value id="1" label="Basemap Imagery" metacontent="baseimagery"/>
     <value id="2" label="Multispectral Imagery" metacontent="multispectral"/>
		<value id="6" label="None" metacontent="imagery"/>
	  <value id="4" label="Temporal Imagery" metacontent="temporal"/>
	  <value id="5" label="Event Imagery" metacontent="event_imagery"/>
    </filter>
    <filter id="2" label="Basemaps" metaname="basemap">
      <value id="1" label="Esri Basemaps" metacontent="esri_basemap"/>
	  <value id="2" label="Partner Basemaps" metacontent="partner_basemap"/>
	  <value id="3" label="User Basemaps" metacontent="user_basemap"/>
	  <value id="4" label="None" metacontent="basemap"/>
    </filter>
    <filter id="3" label="Historical Maps" metaname="historical">
      <value id="1" label="None" metacontent="historical"/>
    </filter>
	
	<filter id="4" label="Demographics and Lifestyle" metaname="demographics">
	  <value id="1" label="Income" metacontent="income"/>
	  <value id="2" label="Spending and Behaviors" metacontent="spending or behaviors"/>
	  <value id="3" label="Population and Housing" metacontent="population or housing"/>
	  <value id="4" label="Segmentation" metacontent="segmentation"/>
	  <value id="5" label="Business and Jobs" metacontent="business or jobs"/>
	  <value id="6" label="Poverty or At Risk" metacontent="poverty or at risk"/>
    </filter>
	<filter id="5" label="Landscape" metaname="landscape">
	  <value id="1" label="Climate and Weather" metacontent="climate or weather"/>
	  <value id="2" label="Ecology" metacontent="ecology"/>
	  <value id="3" label="Species Biology" metacontent="species"/>
	  <value id="4" label="Disturbance and Impact" metacontent="disturbance"/>
      <value id="5" label="Elevation" metacontent="elevation"/>
