css:
  - homepage
  - downloads

js:
  - download

metatags:
  - <META name="description" content="AppStudio for ArcGIS"><META name="last-modified" content="2015-3-3"><META name="product" content="appstudio,appstudio-browser"><META name="sub_category" content="AppStudio"><META name="search-collection" content="help"><META name="content_type_label" content="Help Topic"><META name="sub_category_label" content="AppStudio"><META name="search-category" content="help-general"><META name="subject_label" content="APPS,WEB APPLICATIONS,WEB,ARCGIS ONLINE"><META name="version" content=""><META name="content_type" content="Help Topic"><META name="subject" content="APPS,WEB APPLICATIONS,WEB,ARCGIS ONLINE"><META name="content-language" content="zh-hk">

mobile: true

---


<header class="sub-nav sub-nav-blue">
  <div class="grid-container">
    <div class="column-24">
      <div style="display:table;" class="sdk-home-banner">
        <div class="sdk-home-banner-content">
          <h1 class="sub-nav-title leader-1 trailer-1 text-white">AppStudio for ArcGIS</h1>
        </div>
        <!-- INSTRUCTIONS: Remove this div tag if the a and p tags within are not needed -->
        <div class="search-bar">
      	  <form id="helpSearchForm" target="_top">
      		  <input type="search" name="q" placeholder="搜尋 AppStudio for ArcGIS" class="leader">
	          <input type="hidden" name="collection" value="help">
	          <input type="hidden" name="product" value="appstudio">
	          <input type="hidden" name="version" value="">
	          <input type="hidden" name="language" value="zh-hk">
	          <button class="search-submit icon-ui-search" type="submit"></button>
      		</form>
      	</div>
      </div>
    </div>
    <div class="column-24">
      <select class="tablet-show select-full trailer-half js-select-nav">
        <option value="/en/appstudio/">首頁</option>
        <option value="/en/appstudio/create-apps/">建立應用程式</option>
        <option value="/en/appstudio/extend-apps/">延伸應用程式</option>
        <option value="/en/appstudio/api-guide/">API 指南</option>
        <option value="http://links.esri.com/appstudio/apireference">API 參考</option>
      </select>
      <nav class="sub-nav-list tablet-hide">
        <a class="sub-nav-link" href="/zh-hk/appstudio/">首頁</a> 
	<a class="sub-nav-link" href="/zh-hk/appstudio/create-apps/">建立應用程式</a> 
	<a class="sub-nav-link" href="/zh-hk/appstudio/extend-apps/">延伸應用程式</a> 
	<a class="sub-nav-link" href="/zh-hk/appstudio/api-guide/">API 指南</a> 
	<a class="sub-nav-link" href="http://links.esri.com/appstudio/apireference">API 參考</a>
      </nav>
    </div>
  </div>
</header>

<div class="grid-container">
  <div class="column-12 post-1 trailer-2 secured-org downloads">
    <header class="clearfix leader-1">
      <h2 class="left trailer-half font-size-3" id="AppStudio">AppStudio</h2>
      <div class="dropdown js-dropdown right">
        <%= generateDevVersionDropDown("appstudio~secured~windows64") %>
      </div>
    </header>
    <hr class="leader-0 trailer-half">
    <p class="trailer-half">下載 AppStudio for ArcGIS (Desktop Edition)，在您的應用程式上執行進階的自訂或開發工作。</p>
    <div class="clearfix">
      <%= generateVersionButtons("appstudio~secured~windows64") %>
      <%= generateVersionButtons("appstudio~secured~windows86") %>
      <%= generateVersionButtons("appstudio~secured~mac") %>
      <%= generateVersionButtons("appstudio~secured~linux") %>
    </div>
  </div>
  <div class="column-11 trailer-2">
    <header class="clearfix leader-1">
      <h2 class="left trailer-half font-size-3" id="AppStudioPlayer">AppStudio Player</h2>
      <!--div class="dropdown js-dropdown right">
        <a href="#AppStudioPlayer" class="btn btn-clear btn-small" aria-expanded="false">Version 1.3</a>
      </div-->
    </header>
    <hr class="leader-0 trailer-half">
    <p class="trailer-half">先在裝置上下載 AppStudio Player 以預覽應用程式，然後再建立。</p>
    <div class="clearfix">
      <%= generateVersionButtons("appstudio~nonSecured~windows64") %>
      <%= generateVersionButtons("appstudio~nonSecured~windows86") %>
      <%= generateVersionButtons("appstudio~nonSecured~mac") %>
      <%= generateVersionButtons("appstudio~nonSecured~linux") %>
    </div>
    <div class="leader-half">
      <a href="<%= generateVersionButtons("appstudio~nonSecured~ios", false, true) %>" class="download-link" style="margin:.375rem 0"><img src="/assets/img/badges/app_store.svg" style="height:40px;"></a>
      <a href="<%= generateVersionButtons("appstudio~nonSecured~android", false, true) %>" class="download-link" style="margin:.375rem 0"><img src="/assets/img/badges/google_play.png" style="height:40px;"></a>
    </div>
  </div>
</div>
