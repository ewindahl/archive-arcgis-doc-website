            <div class="item-content">
              <a href="#">
                <img src="/en/living-atlas/img/categories-basemaps.png" alt="" />
                <div class="item-desc">
                  <h3>Basemaps</h3>
                  <p><%= lorem.sentences 2 %></p>
                </div>
              </a>
            </div>
          </div>
          <div class="item">
            <div class="item-content">
              <a href="#">
                <img src="/en/living-atlas/img/categories-demographics.png" alt="" />
                <div class="item-desc">
                  <h3>Demographics and Lifestyle</h3>
