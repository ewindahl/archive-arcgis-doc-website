<script src="/en/asset/js/rssfeed2.js" type="text/javascript"></script>
<script type="text/javascript">
  $(window).load(function() {
    $('#blog').rssFeed({
      url:'http://blogs.esri.com/esri/arcgis/category/arcgis-online/feed/',
      max: 3,
      layout: 0
    });

  $("#forum").rssFeed({
    url:"https://geonet.esri.com/community/gis/arcgis-online/view-browse-feed.jspa?browseSite=place-content&browseViewID=placeContent&userID=2188&containerType=14&containerID=2055&filterID=contentstatus%5Bpublished%5D~objecttype~objecttype%5Bthread%5D",
    max: 3,
    layout: 1
    });

  });
</script>
